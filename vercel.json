{
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9"
      }
    },
    {
      "src": "static/**", 
      "use": "@vercel/static-build" 
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)", 
      "dest": "/static/$1" 
    }
  ]
}